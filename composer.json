{
    "name": "bisonlab/sakonnin-bundle",
    "type": "symfony-bundle",
    "description": "A message/log/feedback handling bundle with callback/replies capabilities",
    "keywords": ["logging messages feedback symfony"],
    "homepage": "https://github.com/thomasez/BisonLabSakonninBundle",
    "license": "LGPL-2.1-or-later",
    "authors": [
        {
            "name": "Thomas Lundquist",
            "email": "github@bisonlab.no"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/thomasez/aceeditor-bundle"
        }
    ],
    "require": {
        "ext-imagick": "*",
        "bisonlab/common-bundle": "dev-attributes",
        "bisonlab/context-bundle": "dev-attributes",
        "symfony/framework-bundle": "^6.3 || ^7.0",
        "vich/uploader-bundle": "dev-master",
        "norzechowicz/aceeditor-bundle": "dev-dev-thomasez",
        "stof/doctrine-extensions-bundle": "^1.6",
        "twig/markdown-extra": "^3.3"
    },
    "suggest": {
        "m4tthumphrey/php-gitlab-api": "^11.11"
    },
    "autoload": {
        "psr-4": {
            "BisonLab\\SakonninBundle\\": "src/"
        }
    }
}
